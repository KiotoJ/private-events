<div class="container">
  <h1 class="text-center" action="/events/new">All Events</h1>

  <div class="actions">
    <a href="/events/new">
      New Events
    </a>
  </div>
  <ul>
    <% @events.each do |event| %>
      <li>
        <%= link_to event do %>
          <%= event.event_place %> / <%= event.event_date %>
        <% end %>
      </li>
    <% end %>
  </ul>
</div>
