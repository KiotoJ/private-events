<div class="container">
  <%= link_to 'Back', events_path %>
  <p>
    Place: <%= @event.event_place %>
  </p>
  <p>
    Date: <%= @event.event_date %>
  </p>
  <p>
    Creator: <%= @event.creator.email %>
  </p>
  <%= form_for "/events_attendees/#{@event.id}", method: :patch do |f| %>
    <%= f.submit %>
  <% end %>

  <h3> All attendees </h5>
</div>
